class Solution {
  
   public long multiplyTwoLists(Node l1,Node l2) {
       
          long mod=1000000007;
          long sum1=0;
          long sum2=0;
          
        while(l1!=null){
            sum1=(sum1*10+l1.data)%mod;
            l1=l1.next;
        }
        
        while(l2!=null){
            sum2=(sum2*10+l2.data)%mod;
            l2=l2.next;
        }
         
          return (sum1*sum2)%mod;
   }
}
